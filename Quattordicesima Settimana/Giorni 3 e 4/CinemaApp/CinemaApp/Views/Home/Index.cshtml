@{
    ViewData["Title"] = "Home Page Cinema";
}

<div class="container mt-5">
    <div class="text-center">
        <h1 class="display-4">Benvenuti al Cinema XYZ</h1>
        <p>Scopri i film in programmazione e prenota i tuoi biglietti online.</p>
        <a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a>
    </div>

    <section id="film-in-programmazione" class="mt-5">
        <h2>Film in Programmazione</h2>
        <div class="row">
            <!-- Film 1 -->
            <div class="col-md-4">
                <div class="card">
                    <img src="data:image/jpeg;base64," class="card-img-top" alt="Dune">
                    <div class="card-body">
                        <h5 class="card-title">Dune</h5>
                        <p class="card-text">Un film di fantascienza diretto da Denis Villeneuve, basato sul romanzo di Frank Herbert.</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>

            <!-- Film 2 -->
            <div class="col-md-4">
                <div class="card">
                    <img src="https://m.media-amazon.com/images/I/81IWUfUBwCL._AC_UF1000,1000_QL80_.jpg" class="card-img-top" alt="Top Gun: Maverick">
                    <div class="card-body">
                        <h5 class="card-title">Top Gun: Maverick</h5>
                        <p class="card-text">Il sequel di Top Gun, diretto da Joseph Kosinski e interpretato da Tom Cruise.</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>

            <!-- Film 3 -->
            <div class="col-md-4">
                <div class="card">
                    <img src="https://m.media-amazon.com/images/I/71HHgUHGVYL.jpg" class="card-img-top" alt="Avatar 2">
                    <div class="card-body">
                        <h5 class="card-title">Avatar 2</h5>
                        <p class="card-text">Il sequel del celebre film di James Cameron, ambientato nel mondo di Pandora.</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>

            <!-- Film 4 -->
            <div class="col-md-4 mt-4">
                <div class="card">
                    <img src="https://m.media-amazon.com/images/I/A1aDFIcoowL._AC_UF1000,1000_QL80_.jpg" class="card-img-top" alt="Mission: Impossible 8">
                    <div class="card-body">
                        <h5 class="card-title">Mission: Impossible 8</h5>
                        <p class="card-text">Prossimo capitolo della serie di spionaggio con Tom Cruise.</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>

            <!-- Film 5 -->
            <div class="col-md-4 mt-4">
                <div class="card">
                    <img src="https://pad.mymovies.it/filmclub/2022/09/094/locandinapg1.jpg" class="card-img-top" alt="Inside Out 2">
                    <div class="card-body">
                        <h5 class="card-title">Inside Out 2</h5>
                        <p class="card-text">"Emozioni esplorano nuove avventure nel labirinto della crescita emotiva."</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>

            <!-- Film 6 -->
            <div class="col-md-4 mt-4">
                <div class="card">
                    <img src="https://i.ebayimg.com/images/g/--UAAOSwj2ll0DZe/s-l1600.jpg" class="card-img-top" alt="deadpool and wolverine">
                    <div class="card-body">
                        <h5 class="card-title">Deadpool and Wolverine</h5>
                        <p class="card-text">"Deadpool e Wolverine uniscono forze per combattere un nemico comune, mescolando umorismo e azione selvaggia."</p>
                        <a href="#" class="btn btn-primary">Dettagli</a>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <section id="orari-proiezioni" class="mt-5">
        <h2>Orari Proiezioni</h2>
        <table class="table">
            <thead>
                <tr>
                    <th>Film</th>
                    <th>Data</th>
                    <th>Ora</th>
                    <th>Sala</th>
                    <th>Azioni</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Interstellar</td>
                    <td>15/10/2023</td>
                    <td>19:00</td>
                    <td>SALA NORD</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
                <tr>
                    <td>Il Signore degli Anelli: La Compagnia dell'Anello</td>
                    <td>16/10/2023</td>
                    <td>20:30</td>
                    <td>SALA EST</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
                <tr>
                    <td>Inception</td>
                    <td>17/10/2023</td>
                    <td>21:00</td>
                    <td>SALA SUD</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
                <tr>
                    <td>Dune</td>
                    <td>15/01/2024</td>
                    <td>18:30</td>
                    <td>SALA NORD</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
                <tr>
                    <td>Top Gun: Maverick</td>
                    <td>16/01/2024</td>
                    <td>20:00</td>
                    <td>SALA EST</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
                <tr>
                    <td>Avatar 2</td>
                    <td>17/01/2024</td>
                    <td>21:30</td>
                    <td>SALA SUD</td>
                    <td><a class="btn btn-primary" asp-controller="Tickets" asp-action="Index">Prenota</a></td>
                </tr>
            </tbody>
        </table>
    </section>
</div>
